     "execution_count": 35,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Create urban/rural categories based on population\n",
    "\n",
    "def categorize_urban(pop):        # Bins of population, by quantity\n",
    "    if pd.isna(pop):\n",
    "        return 'Unknown'\n",
    "    elif pop < 2000:\n",
    "        return 'Rural (<2k)'\n",
    "    elif pop < 10000:\n",
    "        return 'Small Urban (2k-10k)'\n",
    "    elif pop < 50000:\n",
    "        return 'Medium Urban (10k-50k)'\n",
    "    else:\n",
    "        return 'Large Urban (>50k)'\n",
    "\n",
    "\n",
    "\n",
    "commune_stats['urban_category'] = commune_stats['population'].apply(categorize_urban)\n",
    "\n",
    "\n",
    "\n",
    "urban_stats = commune_stats.groupby('urban_category').agg(\n",
    "    communes=('code_insee', 'nunique'),\n",
    "    total_venues=('venues', 'sum'),\n",
    "    total_population=('population', 'sum')\n",
    ")\n",
    "\n",
    "\n",
    "urban_stats['venues_per_commune'] = urban_stats['total_venues'] / urban_stats['communes']\n",
    "urban_stats['density_per_100k'] = (urban_stats['total_venues'] / urban_stats['total_population']) * 100000\n",
    "\n",
    "print(f\"\\nDistribution by urban category : {urban_stats}\")\n",
    "\n",
    "\n",
    "urban_stats.sort_values('density_per_100k', ascending=False)\n",
    "        "
   ]
  },
