    "print(f\"\\nRest of France communes:\")\n",
    "print(f\"Mean: {other_densities.mean():.2f} per 100k\")\n",
    "print(f\"Median: {other_densities.median():.2f} per 100k\")\n",
    "print(f\"Std: {other_densities.std():.2f}\")"
   ]
