    "\n",
    "# research question 5: paca regional analysis\n",
    "\n",
    "#Identify paca departments\n",
    "\n",
    "\n",
    "# paca departments (provence-alpes-côte d'azur)\n",
    "paca_departments = ['04', '05', '06', '13', '83', '84']\n",
    "\n",
    "# create paca flag based on department code\n",
    "df['is_paca'] = df['dept_code'].isin(paca_departments)\n",
    "\n",
    "print(f\"\\npaca departments: {', '.join(paca_departments)}\")\n",
    "print(f\"   04 = alpes-de-haute-provence\")\n",
    "print(f\"   05 = hautes-alpes\")\n",
    "print(f\"   06 = alpes-maritimes\")\n",
    "print(f\"   13 = bouches-du-rhône\")\n",
    "print(f\"   83 = var\")\n",
    "print(f\"   84 = vaucluse\")\n",
    "\n",
    "\n",
    "# Basic statistics\n",
    "\n",
    "\n",
    "paca_venues = df[df['is_paca']]\n",
    "other_venues = df[~df['is_paca']]\n",
    "\n",
    "\n",
    "print(f\"\\nStatistics PACA:\")\n",
    "print(f\"   venues: {len(paca_venues):,} ({len(paca_venues)/len(df)*100:.1f}%)\")\n",
    "print(f\"   communes: {paca_venues['code_insee'].nunique():,}\")\n",
    "print(f\"   departments: {paca_venues['dept_code'].nunique()}\")\n",
    "\n",
    "print(f\"\\nrest of france:\")\n",
    "print(f\"   venues: {len(other_venues):,} ({len(other_venues)/len(df)*100:.1f}%)\")\n",
    "print(f\"   communes: {other_venues['code_insee'].nunique():,}\")\n",
    "print(f\"   departments: {other_venues['dept_code'].nunique()}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "id": "22a3111a-363f-4727-876f-a69d2968db0d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      "PACA:\n",
      "Communes: 756\n",
      "Total venues: 4,899\n",
      "Total population: 5,172,964\n",
      "Aggregate density: 94.70 per 100k\n",
      "\n",
      "\n",
      "Rest of france:\n",
      "   communes: 21,576\n",
      "   total venues: 78,178\n",
      "   total population: 59,028,408\n",
      "   aggregate density: 132.44 per 100k\n",
      "\n",
      "\n",
      "Comparaison :ratio: 0.72\n",
      "PACA has 28.5% lower density than rest of france\n"
     ]
    }
   ],
   "source": [
    "# Aggregate density by unique communes\n",
    "\n",
    "\n",
    "# paca: group by commune first to get unique population\n",
    "paca_by_commune = paca_venues.groupby('code_insee').agg({\n",
    "    'id': 'count',\n",
    "    'population': 'first'\n",
    "}).rename(columns={'id': 'venues'})\n",
    "\n",
    "paca_by_commune = paca_by_commune[paca_by_commune['population'].notna()]\n",
    "paca_by_commune = paca_by_commune[paca_by_commune['population'] > 0]\n",
    "\n",
    "paca_total_venues = paca_by_commune['venues'].sum()\n",
    "paca_total_population = paca_by_commune['population'].sum()\n",
    "paca_aggregate_density = (paca_total_venues / paca_total_population) * 100000\n",
    "\n",
    "print(f\"\\nPACA:\")\n",
    "print(f\"Communes: {len(paca_by_commune):,}\")\n",
    "print(f\"Total venues: {paca_total_venues:,}\")\n",
    "print(f\"Total population: {paca_total_population:,.0f}\")\n",
    "print(f\"Aggregate density: {paca_aggregate_density:.2f} per 100k\")\n",
    "print()\n",
    "# Rest of france: group by commune first\n",
    "other_by_commune = other_venues.groupby('code_insee').agg({\n",
    "    'id': 'count',\n",
    "    'population': 'first'\n",
    "}).rename(columns={'id': 'venues'})\n",
    "\n",
    "other_by_commune = other_by_commune[other_by_commune['population'].notna()]\n",
    "other_by_commune = other_by_commune[other_by_commune['population'] > 0]\n",
    "\n",
    "other_total_venues = other_by_commune['venues'].sum()\n",
    "other_total_population = other_by_commune['population'].sum()\n",
    "other_aggregate_density = (other_total_venues / other_total_population) * 100000\n",
    "\n",
    "print(f\"\\nRest of france:\")\n",
    "print(f\"   communes: {len(other_by_commune):,}\")\n",
    "print(f\"   total venues: {other_total_venues:,}\")\n",
    "print(f\"   total population: {other_total_population:,.0f}\")\n",
    "print(f\"   aggregate density: {other_aggregate_density:.2f} per 100k\")\n",
    "print()\n",
    "# comparison\n",
    "ratio = paca_aggregate_density / other_aggregate_density\n",
    "diff_pct = ((paca_aggregate_density - other_aggregate_density) / other_aggregate_density) * 100\n",
    "\n",
    "print()\n",
    "print(f\"Comparaison :ratio: {ratio:.2f}\")\n",
    "if ratio > 1:\n",
    "    print(f\"PACA has {diff_pct:.1f}% higher density than rest of france\")\n",
    "else:\n",
    "    print(f\"PACA has {abs(diff_pct):.1f}% lower density than rest of france\")"
   ]
  },
