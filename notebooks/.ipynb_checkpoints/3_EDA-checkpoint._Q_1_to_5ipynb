    "\n",
    "# Define categories in order\n",
    "categories = ['Rural (<2k)', 'Small Urban (2k-10k)', \n",
    "              'Medium Urban (10k-50k)', 'Large Urban (>50k)']\n",
    "\n",
    "\n",
    "# Store results\n",
    "results = []                                            \n",
    "\n",
    "\n",
    "for category in categories:\n",
    "    \n",
    "    \n",
    "    category_data = df[df['urban_category'] == category]\n",
    "    \n",
    "  \n",
    "    category_clean = category_data[category_data['population'].notna()]\n",
    "    category_clean = category_clean[category_clean['population'] > 0]\n",
    "    \n",
    "    if len(category_clean) > 0:\n",
    "       \n",
    "        total_venues = len(category_clean)\n",
    "        total_population = category_clean['population'].sum()\n",
    "        aggregate_density = (total_venues / total_population) * 100000\n",
    "        unique_communes = category_clean['code_insee'].nunique()\n",
    "        result_dict = {\n",
    "            'category': category,\n",
    "            'communes': unique_communes,\n",
    "            'venues': total_venues,\n",
    "            'population': total_population,\n",
    "            'density': aggregate_density}\n",
    "        results.append(result_dict)\n",
    "        \n",
    "        \n",
    "        print(f\"\\n{category}:\")\n",
    "        print(f\"   Communes: {unique_communes:,}\")\n",
    "        print(f\"   Total venues: {total_venues:,}\")\n",
    "        print(f\"   Total population: {total_population:,.0f}\")\n",
    "        print(f\"   Aggregate density: {aggregate_density:.1f} per 100k inhabitants\")\n"
   ]
