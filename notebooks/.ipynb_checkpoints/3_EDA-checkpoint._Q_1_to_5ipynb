    "categories = ['Rural (<2k)', 'Small Urban (2k-10k)', 'Medium Urban (10k-50k)', 'Large Urban (>50k)']\n",
    "\n",
    "\n",
    "aggregate_stats = []\n",
    "\n",
    "for cat in categories:\n",
    "    cat_data = df[df['urban_category'] == cat]\n",
    "    \n",
    "    # Remove rows without population\n",
    "    cat_data_clean = cat_data[cat_data['population'].notna()]\n",
    "    cat_data_clean = cat_data_clean[cat_data_clean['population'] > 0]\n",
    "    \n",
    "    if len(cat_data_clean) > 0:                        #  AGGREGATE: Total venues / Total population\n",
