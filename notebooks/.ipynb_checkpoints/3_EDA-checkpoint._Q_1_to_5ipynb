   "id": "9901b146-dff0-4c46-83e8-9f1742866fdc",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      "Hypotheses:\n",
      "H0: No difference in cultural density between paca and rest of france\n",
      "H1: Significant difference exists\n",
      "\n",
      "T-test:\n",
      "T-statistic: -0.7082\n",
      "P-value: 0.4788171590\n",
      "PACA sample: 756 communes\n",
      "Other sample: 21,576 communes\n",
      "\n",
      "Fail to reject h0 (p >= 0.05)\n"
     ]
    }
   ],
   "source": [
    "# Statistical test (t-test on commune densities)\n",
    "\n",
    "\n",
    "# calculate density per commune for both groups\n",
    "paca_by_commune['density'] = (paca_by_commune['venues'] / paca_by_commune['population']) * 100000\n",
    "other_by_commune['density'] = (other_by_commune['venues'] / other_by_commune['population']) * 100000\n",
    "\n",
    "# remove inf/nan\n",
    "paca_densities = paca_by_commune['density'].replace([np.inf, -np.inf], np.nan).dropna()\n",
    "other_densities = other_by_commune['density'].replace([np.inf, -np.inf], np.nan).dropna()\n",
    "\n",
    "print(\"\\nHypotheses:\")\n",
    "print(\"H0: No difference in cultural density between paca and rest of france\")\n",
    "print(\"H1: Significant difference exists\")\n",
    "\n",
    "\n",
    "# perform t-test\n",
    "t_stat, p_value = st.ttest_ind(paca_densities, other_densities)\n",
    "\n",
    "print(f\"\\nT-test:\")\n",
    "print(f\"T-statistic: {t_stat:.4f}\")\n",
    "print(f\"P-value: {p_value:.10f}\")\n",
    "print(f\"PACA sample: {len(paca_densities):,} communes\")\n",
    "print(f\"Other sample: {len(other_densities):,} communes\")\n",
    "\n",
    "if p_value < 0.05:\n",
    "    print(f\"\\n   reject h0 (p < 0.05)\")\n",
    "    if paca_aggregate_density < other_aggregate_density:\n",
    "        print(f\"PACA has significantly lower cultural density\")\n",
    "    else:\n",
    "        print(f\"PACa has significantly higher cultural density\")\n",
    "else:\n",
    "    print(f\"\\nFail to reject h0 (p >= 0.05)\")\n",
    "   \n",
    "    \n",
    "    # No significant difference\n"
   ]
  },
  {
