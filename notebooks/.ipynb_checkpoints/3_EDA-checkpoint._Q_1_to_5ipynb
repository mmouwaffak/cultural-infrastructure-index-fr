  {
   "cell_type": "code",
   "execution_count": 31,
   "id": "360a00f3-b2d9-4413-bb5d-51cebe7013f6",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Population total (unique communes): 64201372\n",
      "Nb communes: 22332\n"
     ]
    }
   ],
   "source": [
    "# commune level\n",
    "\n",
    "# 1 row per commune (no population duplication)\n",
    "commune_stats = df.groupby('code_insee').agg(\n",
    "    venues=('id', 'count'),\n",
    "    population=('population', 'first'),\n",
    "    dept_code=('dept_code', 'first'),\n",
    "    region_code=('region_code', 'first'),\n",
    "    median_income=('median_income', 'first'),\n",
    "    poverty_rate=('poverty_rate', 'first')\n",
    ").reset_index()\n",
    "\n",
    "# density per commune\n",
    "commune_stats = commune_stats[commune_stats['population'].notna() & (commune_stats['population'] > 0)]\n",
    "commune_stats['density_per_100k'] = (commune_stats['venues'] / commune_stats['population']) * 100000\n",
    "\n",
    "# sanity check: France population should be around ~67M (metropole+DOM, depending on dataset year)\n",
    "print(\"Population total (unique communes):\", int(commune_stats['population'].sum()))\n",
    "print(\"Nb communes:\", commune_stats['code_insee'].nunique())\n"
   ]
  },
