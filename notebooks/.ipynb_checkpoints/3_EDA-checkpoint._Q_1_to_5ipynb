   "source": [
    "# ANOVA on commune densities (variation between communes)\n",
    "\n",
    "\n",
    "valid_data = [d for d in data_for_box if len(d) > 0]\n",
    "\n",
    "if len(valid_data) >= 2:\n",
    "    f_stat, p_value_anova = st.f_oneway(*valid_data)\n",
    "    \n",
    "    print(f\"\\nH0: No difference in commune-level densities across categories\")\n",
    "    print(f\"H1: Significant differences exist\")\n",
    "    print(f\"\\nF-statistic: {f_stat:.2f}\")\n",
    "    print(f\"P-value: {p_value_anova:.10f}\")\n",
    "    \n",
    "    if p_value_anova < 0.05:\n",
    "        print(f\"\\nReject H0 (p < 0.05)\")\n",
    "        print(f\"→ Commune-level densities differ significantly between categories\")\n",
    "      \n",
    "    else:\n",
    "        print(f\"\\nFail to reject H0\")\n",
    "else:\n",
    "    print(\"\\n Not enough groups for ANOVA\")            # at least 2\n",
    "\n",
    "# Note: Mean can be inflated by small communes : BIAS"
   ]
