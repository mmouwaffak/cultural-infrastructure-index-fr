    "        'population': 'first'})                                 # Calculate density by commune\n",
    "    \n",
    "    commune_density = commune_density[commune_density['population'].notna()]\n",
    "    commune_density = commune_density[commune_density['population'] > 0]\n",
    "    commune_density['density'] = (commune_density['id'] / commune_density['population']) * 100000\n",
    "    \n",
    "    density_values = commune_density['density'].replace([np.inf, -np.inf], np.nan).dropna()\n",
    "    \n",
    "    data_for_box.append(density_values)\n",
    "    \n",
    "    if len(density_values) > 0:\n",
    "        print(f\"\\n{cat}:\")\n",
    "        print(f\"   Median commune density: {density_values.median():.1f} per 100k\")\n",
    "        print(f\"   Mean commune density: {density_values.mean():.1f} per 100k\")\n",
    "        \n",
    "\n",
    "# Note: Mean can be inflated by small communes\n",
    "\n"
   ]
