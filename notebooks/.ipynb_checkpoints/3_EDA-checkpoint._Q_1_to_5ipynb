    "# How diverse is the cultural offering across territories?\n",
    "\n",
    "# Observation of departments (diversity)\n",
    "\n",
    "# Calculate diversity by department\n",
    "dept_diversity = df.groupby('dept_code').agg({\n",
    "    'type_equipement_ou_lieu': 'nunique',           # Number of unique types\n",
    "    'id': 'count'}).rename(columns={\n",
    "    'type_equipement_ou_lieu': 'unique_types',\n",
    "    'id': 'total_venues'})\n",
    "\n",
    "# Diversity index (types per 100 venues)\n",
    "dept_diversity['diversity_index'] = (dept_diversity['unique_types'] / dept_diversity['total_venues']) * 100\n",
    "\n",
    "dept_diversity = dept_diversity.sort_values('unique_types', ascending=False)\n",
    "\n",
    "print(f\"\\nOverall statistics\")\n",
    "print(f\"   Total unique venue types in France: {df['type_equipement_ou_lieu'].nunique()}\")\n",
    "print(f\"   Average types per department: {dept_diversity['unique_types'].mean():1f}\")\n",
    "print(f\"   Median types per department: {dept_diversity['unique_types'].median():1f}\")\n",
    "\n",
    "print(f\"\\nTop 10 department (in terms of diversity\")\n",
    "for i, (dept, row) in enumerate(dept_diversity.head(10).iterrows(), 1):\n",
    "    print(f\"{i:2f}. Dept {dept}: {row['unique_types']:3f} types, {row['total_venues']:5,} venues (index: {row['diversity_index']:.1f})\")\n",
    "\n",
    "print(f\"\\n10 least diverse departments\")\n",
    "for i, (dept, row) in enumerate(dept_diversity.tail(10).iterrows(), 1):\n",
